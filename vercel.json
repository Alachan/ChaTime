{
    "version": 2,
    "framework": null,
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.7.3"
        }
    },
    "routes": [
        { "src": "/build/(.*)", "dest": "/public/build/$1" },
        {
            "src": "/(.*\\.(png|jpg|jpeg|gif|ico|svg|webp))",
            "dest": "/public/$1"
        },
        {
            "src": "/(css|js|images|fonts|favicon.ico|robots.txt)/(.*)",
            "dest": "/public/$1/$2"
        },
        { "src": "/api/(.*)", "dest": "/api/index.php" },
        {
            "src": "/(.*)",
            "dest": "/api/index.php",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_URL": "https://cha-time.vercel.app",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie"
    },
    "rewrites": [{ "source": "/api/api/(.*)", "destination": "/api/$1" }],
    "build": {
        "env": {
            "VERCEL_BUILD_SCRIPT": "php artisan route:clear && php artisan config:clear"
        }
    }
}
